{
  "name": "stack-lite",
  "version": "1.0.0",
  "description": "stackover flow lite clone",
  "main": "server.js",
  "scripts": {
    "test:full": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest --reporters default jest-stare --coverage --detectOpenHandles --runInBand --testTimeout=60000",
    "test": "jest",
    "test:watch": "jest --verbose --watchAll",
    "build": "babel src --out-dir dist",
    "lint": "eslint '**/*.js'",
    "start": "env-cmd -f .env babel-node src/index.js",
    "dev": "env-cmd -f .env nodemon --watch src --exec babel-node src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Slam256/stack-lite.git"
  },
  "keywords": [
    "API",
    "EXPRESS",
    "NODE"
  ],
  "author": "SLAM",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Slam256/stack-lite/issues"
  },
  "prettier": {
    "singleQuote": true
  },
  "homepage": "https://github.com/Slam256/stack-lite#readme",
  "dependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/node": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "bcrypt": "^5.0.1",
    "colors": "^1.4.0",
    "connect-mongo": "^4.6.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.1",
    "env-cmd": "^10.1.0",
    "express": "^4.18.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.8.1",
    "mongoose": "^6.5.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.19",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "jest-serial-runner": "^1.2.0",
    "jest-stare": "^2.4.1",
    "supertest": "^6.2.4"
  }
}

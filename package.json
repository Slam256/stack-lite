{
  "name": "stack-lite",
  "version": "1.0.0",
  "description": "stackover flow lite clone",
  "main": "server.js",
  "scripts": {
    "test": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest --reporters default jest-stare --coverage --detectOpenHandles --runInBand --testTimeout=60000",
    "test:watch": "jest --verbose --watchAll",
    "lint": "eslint '**/*.js'",
    "build": "babel src --out-dir dist",
    "start": "babel-node src/index.js",
    "dev": "env-cmd -f .env nodemon --watch src --exec babel-node src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Slam256/stack-lite.git"
  },
  "keywords": [
    "API",
    "EXPRESS",
    "NODE"
  ],
  "author": "SLAM",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Slam256/stack-lite/issues"
  },
  "homepage": "https://github.com/Slam256/stack-lite#readme",
  "dependencies": {
    "@babel/cli": "^7.18.10",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.18.1",
    "nodemon": "^2.0.19",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/node": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "jest-serial-runner": "^1.2.0",
    "jest-stare": "^2.4.1",
    "supertest": "^6.2.4"
  }
}
